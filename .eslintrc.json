{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "react/sort-comp": [0],
    "arrow-parens": "off",
    "react/forbid-prop-types": [0]
  }
}